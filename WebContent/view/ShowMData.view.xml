<mvc:View
   controllerName="sap.ui.demo.wt.controller.ShowMData"
   xmlns="sap.m"
   xmlns:common="sap.ui.commons"
   xmlns:core="sap.ui.core"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:layout="sap.ui.layout"
   xmlns:form="sap.ui.layout.form"
   xmlns:unified="sap.ui.unified"
   xmlns:ux3="sap.ui.ux3"
   xmlns:tnt="sap.tnt"
   xmlns:table="sap.ui.table"
   displayBlock="true">

   <Page title="{i18n>homePageTitle}">
		<tnt:ToolHeader>
        <Button icon="sap-icon://menu2" type="Transparent" >
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <ToolbarSpacer width="20px" />
        <Button text="CreateMData" type="Transparent" press="CreatMasterData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <Button text="ShowMData" type="Transparent" press="ShowMasterData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <Button text="CreateTRPData" type="Transparent" press="CreatTRPData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <tnt:ToolHeaderUtilitySeparator />
        <ToolbarSpacer>
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" minWidth="20px" />
            </layoutData>
        </ToolbarSpacer>
        <Button text="TRP" type="Transparent" press="handleUserNamePress">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
     </tnt:ToolHeader>
     
     <Panel expandable="true" expanded="false" headerText="ResourceCategory">
    	 <layout:VerticalLayout>
			<Button id="Look" text="ResourceCategory" style="Emph" press="LookResourceCategory"/>
			
			<table:Table rows="{path: 'test>/'}" firstVisibleRow="3" visibleRowCount="5" cellClick="mycelltest">
				<table:columns>
					<table:Column>
						<table:label>
							<Label text="ResourceCategory" tooltip="show" />
						</table:label>
						<table:template>
							<common:TextView id="ath" text="{test>key}"/>
						</table:template>
					</table:Column>

				</table:columns>
			</table:Table>
										
			<!--  <table:Table rows="{path: 'test>/'}" firstVisibleRow="3" visibleRowCount="5">
				<table:columns>
					<table:Column>
						<table:label>
							<Label text="ResourceType" tooltip="show" />
						</table:label>
						<table:template>
							<common:TextView text="{test>key}"/>							
						</table:template>
					</table:Column>
				</table:columns>
			</table:Table>-->
			<!-- <Link text="{test>key}" press="test"/> -->
		 </layout:VerticalLayout>
	   </Panel>
	</Page>
	


 </mvc:View>