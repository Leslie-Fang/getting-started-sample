<mvc:View
   controllerName="sap.ui.demo.wt.controller.MainMenu"
   xmlns="sap.m"
   xmlns:common="sap.ui.commons"
   xmlns:core="sap.ui.core"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:layout="sap.ui.layout"
   xmlns:form="sap.ui.layout.form"
   xmlns:unified="sap.ui.unified"
   xmlns:ux3="sap.ui.ux3"
   xmlns:tnt="sap.tnt"
   displayBlock="true">
   <Page title="{i18n>homePageTitle}">
		 <tnt:ToolHeader>
        <Button icon="sap-icon://menu2" type="Transparent" >
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <ToolbarSpacer width="20px" />
        <Button text="CreatMData" type="Transparent" press="CreatMasterData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <Button text="ShowMData" type="Transparent" press="ShowMasterData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <Button text="CreatTRPData" type="Transparent" press="CreatTRPData">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <Button text="Navigate" type="Transparent">
            <layoutData>
                <OverflowToolbarLayoutData priority="Low" />
            </layoutData>
        </Button>
        <tnt:ToolHeaderUtilitySeparator />
        <ToolbarSpacer>
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" minWidth="20px" />
            </layoutData>
        </ToolbarSpacer>
        <Button text="TRP" type="Transparent" press="handleUserNamePress">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
 
    </tnt:ToolHeader>
	</Page>
   
 </mvc:View>