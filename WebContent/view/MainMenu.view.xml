			</layout:HorizontalLayout>
			
			<Text id="CreatMasterData" text=" " class="sapUiTinyMargin"/>
			
			<table:Table title="Forwarding Order" rows="{path: 'test>/'}" visibleRowCountMode="Auto" minAutoRowCount="5"
